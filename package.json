{
  "name": "peerjs-rpc",
  "version": "1.0.2",
  "description": "This is a library based on PeerJS that allow to send RPC messages inside a P2PRoom",
  "main": "dist/main.js",
  "source": "src/index.ts",
  "types": "dist/types.d.ts",
  "scripts": {
    "doInstall": "npm install",
    "test": "jest --config jest.config.js",
    "start": "parcel --no-cache",
    "prebuild": "rimraf dist",
    "build": "parcel build --no-cache",
    "local": "npm run build && npm link",
    "prepack": "npm run build",
    "pack": "npm pack"
  },
  "author": "Isirode",
  "license": "SEE LICENSE IN COPYING",
  "dependencies": {
    "emittery": "^1.0.1",
    "peerjs-room": "https://github.com/isirode/peerjs-room/releases/download/1.3.2/peerjs-room-1.3.2.tgz",
    "rfdc": "^1.3.0"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-typescript-tsc": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "@types/node": "20.1.4",
    "@types/jest": "^29.4.0",
    "events": "3.3.0",
    "parcel": "2.8.3",
    "typescript": "5.0.4",
    "rimraf": "4.1.3",
    "jest": "29.4.2",
    "ts-jest": "29.0.5",
    "ts-mockito": "2.6.1"
  }
}
